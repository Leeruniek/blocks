{
  "name": "@asd14/blocks",
  "version": "0.1.0",
  "description": "A Node.js API framework",
  "homepage": "https://github.com/asd14/blocks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd14/blocks.git"
  },
  "bugs": {
    "url": "https://github.com/asd14/blocks/issues"
  },
  "scripts": {
    "----UTIL-------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "build:docs": "---TBD---",
    "prepare": "npm audit fix && npm test && npm run lint",
    "prepublishOnly": "npm run coverage",
    "----TEST-------------------------------------": "",
    "lint": "eslint src",
    "bench": "node src/core__deep-equal/deep-equal.bench.js",
    "test": "tape src/*.test.js src/**/*.test.js",
    "tdd": "nodemon --watch src --exec \"npm run test | tap-diff\"",
    "coverage:report": "NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "----DEVELOP----------------------------------": "",
    "start": "DEBUG=Blocks:* nodemon examples/basic/index.js"
  },
  "keywords": [],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "src/index.js",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@asd14/m": "0.15.0",
    "@asd14/pluginus": "0.3.0",
    "connect": "3.6.6",
    "cors": "2.8.5",
    "cuid": "2.1.4",
    "debug": "4.1.0",
    "path-to-regexp": "2.4.0"
  },
  "devDependencies": {
    "@codemachiner/eslint-config": "3.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.2.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "6.0.1",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.6",
    "npm-check": "5.9.0",
    "nyc": "13.1.0",
    "prettier": "1.15.2",
    "tap-diff": "0.1.1",
    "tape": "4.9.1"
  }
}
